{
  "name": "zlib-wasm",
  "version": "2.0.0-alpha.2",
  "description": "a wasm generator for zlib",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "author": "jobezbb@163.com",
  "license": "MIT & zlib",
  "keywords": [
    "zlib",
    "WebAssembly"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "private": false,
  "scripts": {
    "build:wasm-v1": "rm -rf ./build/* && node ./bin/build.js && cp -rf ./build/zlib-sample.wasm ./src/web/zlib.wasm",
    "build:wasm-v2": "rm -rf ./src/web/build/* && make web",
    "build:web": "yarn clear && ./node_modules/.bin/webpack --config ./bin/webpack.production.config.js",
    "build:ts": "yarn clear && ./node_modules/.bin/tsc -p .",
    "build:demo": "rm -rf demo/dist/* && ./node_modules/.bin/webpack --config ./bin/webpack.demo.config.js",
    "clear": "rm -rf dist/*",
    "start": "node ./bin/server.js"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "devDependencies": {
    "@types/echarts": "^4.6.1",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "^14.0.5",
    "@types/pako": "^1.0.1",
    "babel-loader": "^8.1.0",
    "base64-loader": "^1.0.0",
    "echarts": "^4.8.0",
    "html-webpack-plugin": "5.5.0",
    "lodash-es": "^4.17.15",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.3",
    "wasm-loader": "^1.3.0",
    "webassembly": "^0.11.0",
    "webpack": "^5.64.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "4.3.1",
    "webpack-merge": "^4.2.2",
    "buffer": "5.6.0",
    "pako": "1.0.11"
  },
  "dependencies": {

  }
}
